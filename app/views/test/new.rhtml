<%= error_messages_for :test_set %>

<h2>Create a New Test</h2>

<% form_for :test_set do |f| -%>
<fieldset>
<ul class="form">
<li>
	<labelfor="name">Name</label>
	<%= f.text_field :name %>
	<%= f.hidden_field :language, :value => @language %>
</li><br/>
<p class="submit"><%= submit_tag 'Create' %></p>
</ul>
</fieldset>
<% end -%>