<h2>Tests</h2>
<ul>
	<% if @discussion %>
		<%= render(:partial => "forum", :locals => { :forum => @discussion }) %>
	<% else %>
		<li><%= link_to "Add Discussion Forum", :controller => :admin, :action => :new_forum, :forum_type => "dis" %>
			<br/> There is no Discussion forum defined for this semester, add one by clicking this link
			</li>
	<% end %>
	<% if @gallery %>
		<%= render(:partial => "forum", :locals => { :forum => @gallery }) %>
	<% else %>
		<li><%= link_to "Add Photo Forum", :controller => :admin, :action => :new_forum, :forum_type => "pho" %><br/> There is no Gallery defined for this semester, add one by clicking this link</li>
	<% end %>

</ul>

<%= link_to_back %>